<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-didomi" version="0.1.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Didomi</name>
    <js-module name="Didomi" src="www/Didomi.js">
        <clobbers target="cordova.plugins.Didomi" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="Didomi">
                <param name="ios-package" value="Didomi" />
            </feature>
        </config-file>
        <source-file src="src/ios/Didomi.m" />
    </platform>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="Didomi">
                <param name="android-package" value="cordova-plugin-didomi.Didomi" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/Didomi.java" target-dir="src/cordova-plugin-didomi/Didomi" />
    </platform>
</plugin>